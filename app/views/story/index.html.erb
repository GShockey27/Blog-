<main class="container">
  <section class="row">
    <div class="col-8 push-4">
      <h2>All Stories</h2>
      <h3><%= link_to "+ Add New Story",new_story_path %></h3>
          <%= form_tag story_path, method: "GET" do %>
          <%= text_field_tag :q, params[:q] %>
          <%= submit_tag "Search" %>
          <% end %>
        <br/>
          <%= render "list" %>
    </div>
  </section>
</main>




